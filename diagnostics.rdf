  <diag:LogFile rdf:about='http://redgates.com/2012/12/diagnostics/component/syslog'>
    <dc:description>
    Standard syslog messages file for most distributions.  Most Unix/Linux distros
    use /var/log/messages as the main log file.  Ubuntu decided to be different and
    call it /var/log/syslog.
    </dc:description>
    <diag:path>/var/log/messages</diag:path>
    <diag:path>/var/log/syslog</diag:path>
  </diag:LogFile>

  <diag:LogFile rdf:about='http://redgates.com/2012/12/diagnostics/component/RedisLog'>
    <dc:description>
    Log files for the appliance database (redis) server.
    </dc:description>
    <diag:path>/var/log/redis.log</diag:path>
  </diag:LogFile>

  <diag:LogFile rdf:about='http://redgates.com/2012/12/diagnostics/component/RedisLogHistory'>
    <dc:description>
    Older log files for the appliance database (redis) server.
    </dc:description>
    <diag:path>/var/log/redis.log.*</diag:path>
  </diag:LogFile>

  <diag:Directory rdf:about='http://redgates.com/2012/12/diagnostics/component/sysconfig'>
    <dc:description>
    RedHat systems have much of their config stored in the /etc/sysconfig
    directory.
    </dc:description>
    <diag:path>/etc/sysconfig</diag:path>
  </diag:Directory>

  <diag:Directory rdf:about='http://redgates.com/2012/12/diagnostics/component/apache'>
    <dc:description>
    Apache config and log files for requests and errors.
    apache2 is ubuntu, httpd is redhat.  This needs to change to use the platform
    config that is used for commands.
    </dc:description>
    <diag:path>/var/log/apache2</diag:path>
    <diag:path>/var/log/httpd</diag:path>
    <diag:path>/etc/httpd</diag:path>
  </diag:Directory>


  <diag:Query rdf:about='http://redgates.com/2012/12/diagnostics/component/InterfaceAddresses'>
    <rdfs:label>InterfaceAddresses</rdfs:label>
    <dc:description>
    List of addresses configured on network interfaces.
    </dc:description>
  </diag:Query>

  <diag:Execute rdf:about="http://redgates.com/2012/12/diagnostics/component/InterfaceAddresses">
      <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Linux_(operating_system)' />
      <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/InterfaceAddresses' />
      <diag:command><![CDATA[ ip address list ]]></diag:command>
  </diag:Execute>

  <diag:Execute rdf:about="http://redgates.com/2012/12/diagnostics/component/InterfaceAddresses">
      <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Darwin_(operating_system)' />
      <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/InterfaceAddresses' />
      <diag:interpreter>bash</diag:interpreter>
      <diag:command><![CDATA[ ifconfig -a ]]></diag:command>
  </diag:Execute>

  <diag:Execute rdf:about="http://redgates.com/2012/12/diagnostics/component/InterfaceAddresses">
      <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Microsoft_Windows' />
      <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/InterfaceAddresses' />
      <diag:interpreter>cmd.exe</diag:interpreter>
      <diag:command><![CDATA[ ipconfig /all ]]></diag:command>
  </diag:Execute>

  <diag:Query rdf:about='http://redgates.com/2012/12/diagnostics/component/NeighborTable'>
    <rdfs:label>NeighborTable</rdfs:label>
    <dc:description>
    List of network neighbors known to the host.
    </dc:description>
  </diag:Query>

  <diag:Execute rdf:parseType='_'>
      <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Linux_(operating_system)' />
      <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/NeighborTable' />
      <diag:command><![CDATA[ ip neigh list ]]></diag:command>
  </diag:Execute>

  <diag:Execute rdf:parseType='_'>
      <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Darwin_(operating_system)' />
      <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/NeighborTable' />
      <diag:command><![CDATA[ arp -a ]]></diag:command>
  </diag:Execute>

  <diag:Query rdf:about='http://redgates.com/2012/12/diagnostics/component/ListeningSockets'>
    <rdfs:label>ListeningSockets</rdfs:label>
    <dc:description>
    List of sockets listening on this host.
    Useful to determine if the service required are actually listening.
    </dc:description>
  </diag:Query>

  <diag:Execute rdf:parseType='_'>
    <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Linux_(operating_system)' />
    <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/ListeningSockets' />
    <diag:command><![CDATA[ netstat -nlp ]]></diag:command>
  </diag:Execute>

  <diag:Query rdf:about='http://redgates.com/2012/12/diagnostics/component/RoutingTable'>
    <rdfs:label>RoutingTable</rdfs:label>
    <dc:description>
    List of network routes present on the appliance.
    Useful to determine connectivity from/to the appliance.
    </dc:description>
  </diag:Query>

  <diag:Execute rdf:parseType='_'>
    <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Linux_(operating_system)' />
    <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/RoutingTable' />
    <diag:command><![CDATA[ ip route list ]]></diag:command>
  </diag:Execute>

  <diag:Execute rdf:parseType='_'>
    <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Darwin_(operating_system)' />
    <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/RoutingTable' />
    <diag:command><![CDATA[ netstat -nr ]]></diag:command>
  </diag:Execute>

  <diag:Query rdf:about='http://redgates.com/2012/12/diagnostics/component/ProcessList'>
    <rdfs:label>ProcessList</rdfs:label>
    <dc:description>
    List of currently running processes.
    </dc:description>
  </diag:Query>

  <diag:Execute rdf:about="_">
      <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Linux_(operating_system)' />
      <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/ProcessList' />
      <diag:command><![CDATA[ ps auxwww ]]></diag:command>
  </diag:Execute>

  <diag:Execute rdf:about="_">
      <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Darwin_(operating_system)' />
      <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/ProcessList' />
      <diag:interpreter>bash</diag:interpreter>
      <diag:command><![CDATA[ ps auxwww ]]></diag:command>
  </diag:Execute>

  <diag:Execute rdf:about="_">
      <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Microsoft_Windows' />
      <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/ProcessList' />
      <diag:interpreter>cmd.exe</diag:interpreter>
      <diag:command><![CDATA[ tasklist /v ]]></diag:command>
  </diag:Execute>

  <diag:Query rdf:about='http://redgates.com/2012/12/diagnostics/component/KernelSettings'>
    <rdfs:label>KernelSettings</rdfs:label>
    <dc:description>
    List of kernel settings.
    </dc:description>
  </diag:Query>

  <diag:Execute rdf:about="_">
    <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Linux_(operating_system)' />
    <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/KernelSettings' />
    <diag:command><![CDATA[ sysctl -a ]]></diag:command>
  </diag:Execute>

  <diag:Query rdf:about='http://redgates.com/2012/12/diagnostics/component/DMIInformation'>
    <rdfs:label>DMIInformation</rdfs:label>
    <dc:description>
    </dc:description>
  </diag:Query>

  <diag:Execute rdf:about="_">
    <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Linux_(operating_system)' />
