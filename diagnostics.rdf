  <diag:Query rdf:about='http://redgates.com/2012/12/diagnostics/component/BIOSInformation'>
    <rdfs:label>BIOSInformation</rdfs:label>
    <dc:description>
    </dc:description>
  </diag:Query>

  <diag:Execute rdf:about="_">
    <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Linux_(operating_system)' />
