  <diag:Query rdf:about='http://redgates.com/2012/12/diagnostics/component/PartitionList'>
    <rdfs:label>PartitionList</rdfs:label>
    <dc:description>
    This only works for OSX - need to have platform specific commands
    for linux we need to get /proc/partitions
    </dc:description>
  </diag:Query>

  <diag:Execute rdf:about="_">
    <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Linux_(operating_system)' />
    <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/PartitionList' />
    <diag:command><![CDATA[ cat /proc/partitions ]]></diag:command>
  </diag:Execute>

  <diag:Execute rdf:about="_">
    <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Darwin_(operating_system)' />
    <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/PartitionList' />
    <diag:command><![CDATA[ diskutil list ]]></diag:command>
  </diag:Execute>


  <diag:Query rdf:about='http://redgates.com/2012/12/diagnostics/component/DiskFreeSpace'>
    <rdfs:label>DiskFreeSpace</rdfs:label>
    <dc:description>
    </dc:description>
  </diag:Query>

  <diag:Execute rdf:about="_">
    <dc:description>
      This should be generic Unix os.
    </dc:description>
    <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Unix-like' />
    <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Linux_(operating_system)' />
    <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Darwin_(operating_system)' />
    <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/DiskFreeSpace' />
    <diag:command><![CDATA[ df -k ]]></diag:command>
  </diag:Execute>


  <diag:Query rdf:about='http://redgates.com/2012/12/diagnostics/component/KernelModules'>
    <rdfs:label>KernelModules</rdfs:label>
    <dc:description>
    </dc:description>
  </diag:Query>

  <diag:Execute rdf:about="_">
    <dc:description>
        Need to add a real rdf:about.
        Perhaps add a "match" element that helps identify the host?
    </dc:description>
    <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Linux_(operating_system)' />
    <diag:uname>Linux</diag:uname>
    <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/KernelModules' />
    <diag:command><![CDATA[ lsmod ]]></diag:command>
  </diag:Execute>

  <diag:Execute rdf:about="_">
    <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Darwin_(operating_system)' />
    <diag:uname>Darwin</diag:uname>
    <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/KernelModules' />
    <diag:command><![CDATA[ kextstat ]]></diag:command>
  </diag:Execute>


  <diag:Query rdf:about='http://redgates.com/2012/12/diagnostics/component/RecentLogins'>
    <rdfs:label>RecentLogins</rdfs:label>
    <dc:description>
    List of the most recent logins on the console or ssh.  For Unix this is the "last"
    command.  For Windows TBD.
    </dc:description>
  </diag:Query>

  <diag:Execute rdf:about="_">
      <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Linux_(operating_system)' />
      <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/RecentLogins' />
      <diag:command><![CDATA[ last ]]></diag:command>
  </diag:Execute>


  <diag:Query rdf:about='http://redgates.com/2012/12/diagnostics/component/EventLog'>
    <rdfs:label>EventLog</rdfs:label>
    <dc:description>
    Report windows events from the log.
    There are multiple kinds of events, change this to be more specific and
    then add it to one of the Archives.
    </dc:description>
  </diag:Query>

  <diag:Execute rdf:about="_">
      <diag:forPlatform rdf:resource='http://dbpedia.org/resource/Microsoft_Windows' />
      <diag:answersQuery rdf:resource='http://redgates.com/2012/12/diagnostics/component/EventLog' />
      <diag:interpreter>cmd.exe</diag:interpreter>
      <diag:command><![CDATA[ wevtutil qu logname params ]]></diag:command>
  </diag:Execute>


  <diag:Archive rdf:about='http://redgates.com/2012/12/diagnostics/Archive/Base'>
    <rdfs:label>base</rdfs:label>
    <dc:description>
    Base Diagnostic archive included by all others.
    </dc:description>
    <diag:component rdf:resource='http://redgates.com/2012/12/diagnostics/component/BIOSInformation' />
    <diag:component rdf:resource='http://redgates.com/2012/12/diagnostics/component/DMIInformation' />
    <diag:component rdf:resource='http://redgates.com/2012/12/diagnostics/component/KernelModules' />
    <diag:component rdf:resource='http://redgates.com/2012/12/diagnostics/component/SysCtl' />
    <diag:component rdf:resource='http://redgates.com/2012/12/diagnostics/component/ProcessList' />
    <diag:component rdf:resource='http://redgates.com/2012/12/diagnostics/component/syslog' />
  </diag:Archive>

  <diag:Archive rdf:about='http://redgates.com/2012/12/diagnostics/Archive/Network'>
    <rdfs:label>network</rdfs:label>
    <dc:description>
    Diagnostic information about current network configuration.
    </dc:description>
    <diag:component rdf:resource='http://redgates.com/2012/12/diagnostics/component/ListeningSockets' />
    <diag:component rdf:resource='http://redgates.com/2012/12/diagnostics/component/InterfaceAddresses' />
  </diag:Archive>

  <diag:Archive rdf:about='http://redgates.com/2012/12/diagnostics/Archive/Storage'>
    <rdfs:label>storage</rdfs:label>
    <dc:description>
    Diagnostic information about current storage configuration.
    </dc:description>
    <diag:component rdf:resource='http://redgates.com/2012/12/diagnostics/component/PartitionList' />
    <diag:component rdf:resource='http://redgates.com/2012/12/diagnostics/component/DiskFreeSpace' />
  </diag:Archive>

  <diag:Archive rdf:about='http://redgates.com/2012/12/diagnostics/Archive/SecurityAudit'>
    <rdfs:label>storage</rdfs:label>
    <dc:description>
    Diagnostic information about logins and permissions and other items
    related to security.
    </dc:description>
    <diag:component rdf:resource='http://redgates.com/2012/12/diagnostics/component/RecentLogins' />
  </diag:Archive>

  <diag:Archive rdf:about='http://redgates.com/2012/12/diagnostics/Archive/Web'>
    <rdfs:label>storage</rdfs:label>
    <dc:description>
    Diagnostic information about web services, eg. apache.
    </dc:description>
    <diag:component rdf:resource='http://redgates.com/2012/12/diagnostics/component/apache' />
  </diag:Archive>

  <diag:Archive rdf:about='http://redgates.com/2012/12/diagnostics/Archive/History'>
    <rdfs:label>storage</rdfs:label>
    <dc:description>
    Historical log files - generated by the logrotate process.
    </dc:description>
    <diag:component rdf:resource='http://redgates.com/2012/12/diagnostics/component/RedisLogHistory' />
  </diag:Archive>

  <diag:Archive rdf:about='http://redgates.com/2012/12/diagnostics/Archive/CallHome'>
    <rdfs:label>callhome</rdfs:label>
    <dc:description>
    Diagnostic archive for callhome.
    A callhome diagnostic is used when a problem occurs and the product
    sends a callhome message reporting the problem and delivering
    the diagnostic archive for support to review.
    </dc:description>

